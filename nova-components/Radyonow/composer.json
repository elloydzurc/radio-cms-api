{
    "name": "clay/radyonow",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Clay\\Radyonow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clay\\Radyonow\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
